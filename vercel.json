{
    "version":2,
    "builds": [
        {
            "src":"bvu_sudent_portal/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize": "15mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use": "@vercel/static-build",
            "config":{
                "disDir":"staticfiles_biuld"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src":"/(.)/",
            "dest":"bvu_sudent_portal/wsgi.py"
        }
    ]
}